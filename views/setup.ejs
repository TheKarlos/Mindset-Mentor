<html>

<head>
    <title>Account Setup - Hackathon</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet"
        href="https://cdnjs.cloudflare.com/ajax/libs/tempusdominus-bootstrap-4/5.0.1/css/tempusdominus-bootstrap-4.min.css" />
    <link rel="stylesheet" href="/login.css">
</head>

<body>

    <body>
        <div class="login-form">
            <form action="/examples/actions/confirmation.php" method="post">
                <h2 class="text-center">Setup</h2>
                <div class="form-group">
                    <div class="input-group" id="datetimepicker1" data-target-input="nearest">
                        <div class="input-group-prepend" data-target="#datetimepicker1" data-toggle="datetimepicker">
                            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                        </div>
                        <input type="text" class="form-control datetimepicker-input" data-target="#datetimepicker1"
                            placeholder="Date of birth" />

                    </div>
                </div>
                <div class="form-group" id="genderSelectGroup">
                    <label class="form-group-label" for="genderSelectGroup">Gender:</label>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="genderSelect" id="genderSelectMale"
                            value="male" checked>
                        <label class="form-check-label" for="genderSelectMale">
                            Male
                        </label>
                    </div>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="genderSelect" id="genderSelectFemale"
                            value="female">
                        <label class="form-check-label" for="genderSelectFemale">
                            Female
                        </label>
                    </div>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="genderSelect" id="genderSelectOther"
                            value="other">
                        <label class="form-check-label" for="genderSelectOther">
                            Other
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <label for="problemsSelect">Problems:</label>
                    <select multiple class="form-control" id="problemsSelect">
                        <option>Loneliness</option>
                        <option>Study/Work</option>
                        <option>Family/Home</option>
                        <option>Depression</option>
                        <option>Sleep</option>
                        <option>Obesity</option>
                        <option>Addiction</option>
                        <option>Stress/Anxiety</option>
                        <option>Bullying</option>
                        <option>Diseases</option>
                        <option>Discrimination</option>
                        <option>Body Image</option>
                    </select>
                </div>
                <div class="form-group">
                        <label for="likesSelect">Likes:</label>
                        <select multiple class="form-control" id="likesSelect">
                            <option>Social/Games</option>
                            <option>Sport</option>
                            <option>Learning</option>
                            <option>Music</option>
                            <option>Outdoor</option>
                            <option>Creative</option>
                            <option>Hobbies</option>
                        </select>
                    </div>
                <div class="form-group">
                    <button type="submit" class="btn btn-success btn-block">Submit</button>
                </div>
            </form>
        </div>
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
            integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
            crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
            integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
            crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
            integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
            crossorigin="anonymous"></script>
        <script type="text/javascript"
            src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js"></script>
        <script type="text/javascript"
            src="https://cdnjs.cloudflare.com/ajax/libs/tempusdominus-bootstrap-4/5.0.1/js/tempusdominus-bootstrap-4.min.js"></script>
    </body>

</html>